;;; begin compile.rsp

(val &opcodes "Set of opcodes used by the bytecode interpreter ((:opcode &opcodes) to get the opcode's value."
  { :push, :pop, :dup,
  
    :loadn, :loadt, :loadf,

    :loadv, :loadg, :storeg,

    :loadl, :storel, :loadc, :storec,

    :jump, :jumpt, :jumpf, :goto, :gotot, :gotof,

    :argc, :vargc,

    :call, :tail-call, :return, :closure, :capture })



;;; end compile.rsp