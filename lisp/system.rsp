;;; begin system.rsp

;;; rascal-side definitions of object flags
(val *cons-flags*
  {
    :cons-flag-proper       =>  1,
    :cons-flag-cycle        =>  2,
    :cons-flag-entry        =>  4,
    :cons-flag-closure      =>  8,
    :cons-flag-continuation => 16
  })

(val *func-flags*
  {
  
  })

;;; standard type predicates
(fun cons? (x)
  (isa? x cons))

(fun nil? (x)
  (id? x nil))

(fun list? (x)
  (isa? x list))

(fun sym? (x)
  (isa? x ))