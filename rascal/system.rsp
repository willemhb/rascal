;; begin system.rsp

;; basic syntax examples

(enum List
      (data Nil)
      (data Cons Any List))

(enum Gender :Male,
      	     :Female,
    	     :NonBinary,
    	     :Other)

(enum Tree (data Empty)
      	   (data Leaf Any)
	   (data Node Any Tree Tree))

(union Number Real,
       	      Int,
 	      Complex,
 	      Ratio)

(class Person
 (:name: Str)
 (:age:  Int)
 (:gender: Gender)
 (birthday: Person [p: Person]))

;; end system.rsp