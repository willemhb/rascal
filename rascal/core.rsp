;;; begin core.rsp
;;; rascal basics module.

;;; basic types
(union list
  (cons nul))

(fun prin
  ((port port) (xs list))
  (where ((loop port xs)
  	  (if (cons? xs) (do (prin port (car xs)) (princ port \space) (loop port (cdr xs)))
	      xs     	 (prins " . #{xs}"))
	      otherwise  (do-nothing))
	 (do (princ \()
	     (loop xs))))

;;; end core.rsp